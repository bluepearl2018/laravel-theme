{
  "name": "eutranet/laravel-theme",
  "description": "Eutranet theme ships with all Eutranet default components, css...",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Eutranet\\Theme\\": "src/",
      "Eutranet\\Theme\\Database\\Seeders\\": "database/seeders/",
      "Eutranet\\Theme\\Database\\Factories\\": "database/factories/"
    }
  },
  "authors": [
    {
      "name": "St√©phane Maissin",
      "email": "stephane.maissin@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Eutranet\\Theme\\ThemeServiceProvider"
      ]
    },
    "aliases": {
      "ThemeMiddleware": "Eutranet\\Theme\\Http\\Controllers\\ThemeMiddleware",
      "BackMenu": "Eutranet\\Theme\\Models\\BackMenu",
      "UserMenu": "Eutranet\\Theme\\Models\\UserMenu",
      "SetupMenu": "Eutranet\\Theme\\Models\\SetupMenu"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^7.3",
    "nunomaduro/collision": "^6.0",
    "nunomaduro/larastan": "^2.0.1",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "laracasts/flash": "^3.2",
    "spatie/laravel-feed": "^4.1",
    "spatie/laravel-ray": "^1.26",
    "spatie/laravel-medialibrary": "^10.0",
    "spatie/laravel-permission": "^5.5",
    "spatie/laravel-translatable": "^5.2",
    "spatie/laravel-translation-loader": "^2.7"
  }
}
